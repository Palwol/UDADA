
public class PrimeNumber {

	public static void main(String[] args) {
		int count = 0;
		
		System.out.print("2부터 100사이 모든 소수:");
		
		for(int k=2; k<=100; k++) {
			count = 0;
			for(int i=2; i<k; i++) {
				if(k%i == 0)
					count++;
					break;
			}
			if(count==0)
				System.out.print(k + "");
		}

	}

}
