
class Account {
	int balance;
	
	public Account() {
		balance = 0;
	}
	
	void withdraw(int amount) {
		balance -= amount;
	}
	
	void deposit(int amount) {
		balance += amount;
	}
}

public class AccountTest {
	public static void main(String args[]) {
		int amount1 = 50000;
		int amount2 = 100000;
		
		Account obj1 = new Account();
		System.out.println("새로운 계좌가 만들어졌습니다.");
		
		obj1.deposit(amount1);
		System.out.println(amount1 + "원 저축");
		
		Account obj2 = new Account();
		System.out.println("새로운 계좌가 만들어졌습니다.");
		
		obj2.deposit(amount2);
		System.out.println(amount2 + "원 저축");
		
		System.out.println("고객 #1의 계좌 잔고=" + obj1.balance);
		System.out.println("고객 #2의 계좌 잔고=" + obj2.balance);
	}

}
