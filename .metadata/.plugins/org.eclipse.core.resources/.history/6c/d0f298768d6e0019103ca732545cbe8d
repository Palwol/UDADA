import java.util.*;

class Editer{
	Stack<String> left = new Stack<String>();
	Stack<String> right = new Stack<String>();
	
	public void L() {
		if(left.empty() == false) {
			String l = left.pop();
			right.push(l);
		}
	}
	
	public void D() {
		if(right.empty() == false) {
			String r = right.pop();
			left.push(r);
		}
	}
	
	public void B() {
		left.pop();
	}
	
	public void P(String s) {
		left.push(s);
	}
}

public class EditerTest {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Editer editer = new Editer();
		
		String cmd = sc.nextLine();
		String[] chars = cmd.split("");
		
		for(String c : chars) {
			editer.P(c);
		}
		
		int n = sc.nextInt();
		
		

	}

}
